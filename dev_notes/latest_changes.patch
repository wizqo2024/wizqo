+  if (dayTitle) {
+    // Extract key concepts from day title
+    const titleWords = dayTitle.toLowerCase().split(' ').filter(word => 
+      word.length > 3 && !['day', 'the', 'and', 'for', 'with', 'your'].includes(word)
